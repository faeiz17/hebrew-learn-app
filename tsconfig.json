{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/app": [
        "app/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/atoms": [
        "components/atoms"
      ],
      "@/atoms/*": [
        "components/atoms/*"
      ],
      "@/molecules": [
        "components/molecules"
      ],
      "@/molecules/*": [
        "components/molecules/*"
      ],
      "@/organisms": [
        "components/organisms"
      ],
      "@/organisms/*": [
        "components/organisms/*"
      ],
      "@/templates": [
        "components/templates"
      ],
      "@/templates/*": [
        "components/templates/*"
      ],
      "@/hooks": [
        "hooks"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/context": [
        "context"
      ],
      "@/context/*": [
        "context/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/types": [
        "types/index.ts"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/api": [
        "utils/api"
      ],
      "@/api/*": [
        "utils/api/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
